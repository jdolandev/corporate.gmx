          <argument>
            <kind>1</kind>
            <string>var accel, fric;
if (on_ground) {
    accel = S_RUN_ACCEL;
    fric = S_RUN_FRIC;
} else {
    accel = S_AIR_ACCEL;
    fric = S_AIR_FRIC;
}


if (keyboard_check( k_right ) || gamepad_axis_value(0, j_right) ) {
    //Running right
    sprite_index = spr_playerR;
    image_speed = 0.25;
    //First add friction if currently running left
    if (hspd &lt; 0)
        hspd = approach( hspd, 0, fric );
        
    hspd = approach( hspd, S_MAX_H, accel ); 

} else if (keyboard_check( k_left ) || gamepad_axis_value( 0, j_left ) &lt; -0.25) {
    //Running left
    sprite_index = spr_playerL;
    image_speed = 0.25;
    //First add friction if currently running right
    if (hspd &gt; 0)
        hspd = approach( hspd, 0, fric );
        
    hspd = approach( hspd, -S_MAX_H, accel ); 

} else {
    //Stopping
    image_speed = 0;
    if( sprite_index == spr_playerR ){
        sprite_index = spr_playerSR;
    }
    else if( sprite_index == spr_playerL ){
        sprite_index = spr_playerSL;
    }
    image_index = 0;
    hspd = approach( hspd, 0, fric );

}


if (on_ground) {

    candj = true;
    
    //Jumping
    if ( keyboard_check_pressed( k_jump ) or gamepad_button_check( 0, j_a ))
        vspd = S_JUMP_SPEED;

} else {

    //Gravity
    vspd = approach( vspd, S_MAX_V, S_GRAVITY );
}
